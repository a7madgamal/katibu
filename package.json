{
  "name": "katibu",
  "productName": "katibu",
  "version": "1.4.1-beta",
  "description": "THE helper tool you need for modern development",
  "repository": "https://github.com/a7madgamal/katibu",
  "main": ".webpack/main",
  "scripts": {
    "start": "yarn tsc && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "electron-forge publish",
    "lint:css": "stylelint './src/**/*.tsx'",
    "tsc": "tsc --noEmit",
    "prettier": "prettier --write src"
  },
  "keywords": [
    "git",
    "jira",
    "github",
    "productivity",
    "electron"
  ],
  "author": {
    "name": "Ahmed Hassanein",
    "email": "eng.a7mad.gamal@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "extendInfo": "Info.plist"
      },
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "a7madgamal",
              "name": "katibu"
            },
            "prerelease": true
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "katibu"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "port": 9898,
            "loggerPort": 1234,
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@octokit/rest": "^17.1.4",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "body-parser": "^1.19.0",
    "compare-versions": "^3.6.0",
    "devtron": "^1.4.0",
    "electron-redux": "^1.5.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-unhandled": "^3.0.2",
    "express": "4.17.1",
    "file-loader": "^6.0.0",
    "final-form": "^4.18.7",
    "final-form-arrays": "^3.0.2",
    "got": "^10.7.0",
    "history": "^4.10.1",
    "jira-connector": "^3.1.0",
    "node-fetch": "^2.6.0",
    "node-jenkins": "^1.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.3.5",
    "react-final-form-arrays": "^3.1.1",
    "react-hot-loader": "^4.12.20",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "simple-git": "^2.10.0",
    "source-map-support": "^0.5.16",
    "ts-loader": "^6.2.2",
    "universal-user-agent": "5.0.0",
    "update-electron-app": "^1.5.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.51",
    "@electron-forge/maker-deb": "6.0.0-beta.51",
    "@electron-forge/maker-rpm": "6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "6.0.0-beta.51",
    "@electron-forge/maker-zip": "6.0.0-beta.51",
    "@electron-forge/plugin-webpack": "6.0.0-beta.51",
    "@electron-forge/publisher-github": "6.0.0-beta.51",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/body-parser": "^1.19.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/express": "4.17.3",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/request": "^2.48.4",
    "@types/source-map-support": "^0.5.1",
    "@types/vfile-message": "^2.0.0",
    "babel-plugin-emotion": "^10.0.29",
    "css-loader": "^3.4.2",
    "electron": "9.0.4",
    "electron-devtools-installer": "^3.0.0",
    "electron-timber": "^0.5.1",
    "node-loader": "^0.6.0",
    "prettier": "^2.0.5",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "type-fest": "^0.12.0",
    "typescript": "^3.9.5"
  }
}
